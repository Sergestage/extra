function flatten(arr) {
  let result = [];
   for (let i = 0; i < arr.length; i++) {
   if (Array.isArray(arr[i])) {
    let flat = flatten(arr[i])
      for (let j = 0; j < flat.length; j++) {
         result.push(flat[j]);
      }
     } else {
       result.push(arr[i]);
    }
   }
    return result;
}

let list = [1, [2, [3, 4]], 5];
console.log(flatten(list));
