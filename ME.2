function hasVowel(word) {
 let vowels = ['a', 'e', 'i', 'o', 'u'];
  for(let i = 0; i < word.length; i++) {
   for(let j = 0; j < vowels.length; j++) {
    if(word[i].toLowerCase() === vowels[j]) {
     return true;
    }
    }
  }
 return false;
}

console.log(hasVowel("sky"));
console.log(hasVowel("apple"));
